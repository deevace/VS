  const navbar = document.getElementById("navbar");
  let lastScrollTop = window.pageYOffset || document.documentElement.scrollTop;

  window.addEventListener("scroll", function () {
    let scrollTop = window.pageYOffset || document.documentElement.scrollTop;

    if (scrollTop > lastScrollTop) {
      // вниз — прячем
      navbar.style.transform = "translateY(-100%)";
    } else {
      // вверх — показываем
      navbar.style.transform = "translateY(0)";
    }

    lastScrollTop = scrollTop <= 0 ? 0 : scrollTop;
  });
</script>

<style>
  #navbar {
    transition: transform 0.3s ease;
    will-change: transform;
  }
</style>
</div></div><div class="scroll"><div class="w-embed"><script>
  let isScrolling = false;
  let currentScroll = window.scrollY;
  let targetScroll = currentScroll;

  const ease = 0.075;

  function smoothScroll() {
    currentScroll += (targetScroll - currentScroll) * ease;
    window.scrollTo(0, currentScroll);
    if (Math.abs(targetScroll - currentScroll) > 0.5) {
      requestAnimationFrame(smoothScroll);
    } else {
      isScrolling = false;
    }
  }

  // Wheel прокрутка
  window.addEventListener('wheel', (e) => {
    e.preventDefault();
    targetScroll += e.deltaY;

    targetScroll = Math.max(0, Math.min(targetScroll, document.body.scrollHeight - window.innerHeight));

    if (!isScrolling) {
      isScrolling = true;
      requestAnimationFrame(smoothScroll);
    }
  }, { passive: false });

  // Фикс для якорных ссылок в навбаре
  document.querySelectorAll('a[href^="#"]').forEach(anchor => {
    anchor.addEventListener('click', function (e) {
      e.preventDefault();

      const id = this.getAttribute('href');
      const targetEl = document.querySelector(id);
      if (targetEl) {
        const offsetTop = targetEl.offsetTop;

        targetScroll = offsetTop;
        if (!isScrolling) {
          isScrolling = true;
          requestAnimationFrame(smoothScroll);
        }
      }
    });
  });
</script>
</div></div><div class="star-bg"><div class="w-embed"><style>
  .star {
    position: absolute;
    width: 2px;
    height: 2px;
    background: white;
    border-radius: 50%;
  }
</style>

<script>
  document.addEventListener("DOMContentLoaded", () => {
    const container = document.querySelector(".star-bg");
    if (!container) return;

    const stars = [];
    const numStars = 100;

    let mouseX = 0;
    let mouseY = 0;

    for (let i = 0; i < numStars; i++) {
      const star = document.createElement("div");
      star.className = "star";
      star.style.top = Math.random() * window.innerHeight + "px";
      star.style.left = Math.random() * window.innerWidth + "px";
      star.depth = Math.random() * 4 + 1;
      container.appendChild(star);
      stars.push(star);
    }

    document.addEventListener("mousemove", (e) => {
      mouseX = (e.clientX / window.innerWidth - 0.5) * 10;
      mouseY = (e.clientY / window.innerHeight - 0.5) * 10;
    });

    function animate() {
      for (const star of stars) {
        const dx = mouseX * star.depth;
        const dy = mouseY * star.depth;
        star.style.transform = `translate(${dx}px, ${dy}px)`;
      }
      requestAnimationFrame(animate);
    }

    animate();
  });
</script>
</div></div><div class="live"><div class="code-embed-7 w-embed w-script"><!-- AOS CSS -->
<link href="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.css" rel="stylesheet">

<!-- AOS JS -->
<script src="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.js"></script>

<script>
  document.addEventListener('DOMContentLoaded', function() {
    AOS.init({
      once: true,           // анимация срабатывает только один раз, когда элемент в поле зрения
      duration: 1200,       // время анимации в мс
      easing: 'ease-out-cubic'  // плавность анимации
    });
  });
</script></div></div><script src="js/jquery.js?site=681f345eb12406d6efb4ab51" type="text/javascript"  ></script><script src="js/webflow.schunk.36b8fb49256177c8.js" type="text/javascript"></script><script src="js/webflow.schunk.50d850aa4962b686.js" type="text/javascript"></script><script src="js/webflow.schunk.b4435221be879eb3.js" type="text/javascript"></script><script src="js/webflow-script.js" type="text/javascript"></script></body></html>